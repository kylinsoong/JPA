<?xml version="1.0" encoding="UTF-8"?>

<persistence version="2.0"
   xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="
        http://java.sun.com/xml/ns/persistence
        http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
   <persistence-unit name="com.kylin.man.po">
      <provider>org.hibernate.ejb.HibernatePersistence</provider>  
      
      <class>com.kylin.man.po.Man</class>
      <class>com.kylin.man.po.Event</class>
      <class>com.kylin.man.po.Friend</class>
      <class>com.kylin.man.po.UserCard</class>
      <class>com.kylin.man.po.Wife</class>
      <class>com.kylin.man.po.Property</class>
      <class>com.kylin.man.po.Pet</class>
      <class>com.kylin.man.po.Audit</class>
      <properties>
      	<property name="hibernate.dialect" value="org.hibernate.dialect.Oracle10gDialect"/>
      </properties>
   </persistence-unit>
</persistence>
